@@ -671,3 +671,10 @@ CONFIG_SCHED_STACK_END_CHECK=y
 CONFIG_IPC_LOGGING=y
 CONFIG_BUG_ON_DATA_CORRUPTION=y
 CONFIG_CC_WERROR=y
+CONFIG_TCP_CONG_ADVANCED=y
